# API Keys (optional - for external models)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
COHERE_API_KEY=your_cohere_key_here
HUGGINGFACE_API_KEY=your_hf_key_here

# VS Code Server
VSCODE_PASSWORD=secure_password_here

# Database
DATABASE_URL=postgresql://postgres:password@postgres:5432/coding_agent

# Redis
REDIS_URL=redis://redis:6379

# Model Preferences
DEFAULT_MODEL_PROVIDER=ollama
DEFAULT_CODE_MODEL=codellama:13b
DEFAULT_CHAT_MODEL=mistral:7b
DEFAULT_ANALYSIS_MODEL=gpt-4

# Features
ENABLE_GPU=true
ENABLE_STREAMING=true
ENABLE_MODEL_SWITCHING=true

# Ollama Configuration
OLLAMA_HOST=http://ollama:11434
OLLAMA_ORIGINS=*

# Development
NODE_ENV=development
LOG_LEVEL=info